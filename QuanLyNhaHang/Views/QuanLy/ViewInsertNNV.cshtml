@model QuanLyNhaHang.Models.NhomNhanVien
@{
    QuanLyNhaHangContext context = new QuanLyNhaHangContext();
    List<Sanh> getListSanh()
    {
        return context.Sanh.Where(x => x.Active == true).OrderByDescending(x => x.Idsanh).ToList();
    }
}

<div class="card">
    <div class="card-body">
        <form action="/QuanLy/InsertNNV" method="post">
            <div class="forms-header text-center">
                <h4 class="forms-title "><b>Thêm nhóm thức ăn</b></h4>
            </div>
            <div class="forms-body">
                <div class="form-group">
                    <label style="margin:7px 0;">Mã nhóm nhân viên</label>
                    <input asp-for="@Model.MaNnv" type="text" class="form-control" required>
                </div>
                <div class="form-group">
                    <label style="margin:7px 0;">Tên nhóm nhân viên</label>
                    <input asp-for="@Model.TenNnv" type="text" class="form-control" required>
                </div>

                <div class="row justify-content-between">
                    <div class="col-sm-2 col-form-label">
                        <button class="btn btn-secondary" type="button" onclick="window.history.go(-1);"> Quay lại</button>
                    </div>
                    <div class="col-sm-auto col-form-label b">
                        <input type="submit" class="btn btn-primary" value="Thêm">
                    </div>

                </div>

            </div>


        </form>
>
    </div>
</div>